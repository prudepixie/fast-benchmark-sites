    "url": "https://github.com/prudepixie/fast-benchmark-sites/issues"
  },
  "homepage": "https://github.com/prudepixie/fast-benchmark-sites#readme",
  "dependencies": {
    "@microsoft/fast-element": "^1.6.2",
    "express": "^4.17.2"
  },
  "devDependencies": {
    "@types/node": "^14.14.37"
  },
  "engines": {
    "node": "16.x"
  }
